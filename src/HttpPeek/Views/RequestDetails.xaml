<UserControl x:Class="HttpPeek.Views.RequestDetails"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             d:DataContext="{StaticResource Design.RequestNodeVm}">
    <UserControl.Resources>
    </UserControl.Resources>
    <Grid>
        <TabControl>
            <TabItem Header="{Binding Body.Title}">

            </TabItem>
            <TabItem Header="{Binding Auth.Title}">

            </TabItem>
            <TabItem Header="{Binding Query.Title}">

            </TabItem>
            <TabItem Header="{Binding Headers.Title}">

            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
